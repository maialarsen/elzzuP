            <Button fx:id="newPuzzle" layoutX="481.0" layoutY="9.0" mnemonicParsing="false" onAction="#goHome" text="New Puzzle" />
            <Label fx:id="puzzleName" layoutX="231.0" layoutY="10.0">
               <font>
                  <Font size="27.0" />
               </font>
            </Label>
         </children>
      </Pane>
   </center>
</BorderPane>
